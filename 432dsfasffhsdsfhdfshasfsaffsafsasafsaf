	SimpleTween(Part, 1, "Linear", "Out", { Color = Color3.new(0,1,1) })
	SimpleTween(Part, 1, "Linear", "Out", { Color = Color3.new(0,0,1) })
	SimpleTween(Part, 1, "Linear", "Out", { Color = Color3.new(1,0,1) })
end

